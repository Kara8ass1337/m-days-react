    "automock": false,
    "testEnvironment": "node",
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.js",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ]
  },
  "repository": "https://github.com/Kara8ass1337/m-days-react",
  "author": "Oleg Shilov",
  "license": "GPLv3",
