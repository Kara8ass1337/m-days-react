{
  "name": "m-days-react",
  "version": "1.0.0",
  "description": "just learning react, take no notice",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --inline --open --config webpack.config.js",
    "build": "cross-env NODE_ENV=production webpack --optimize-minimize",
    "test": "jest --watchAll --coverage"
  },
